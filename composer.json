{
    "name": "nikitakls/yii2-flysystem",
    "description": "The flysystem 2.0 extension for the Yii framework",
    "keywords": [
        "yii2",
        "flysystem",
        "filesystem",
        "files",
        "aws",
        "s3"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nikita Kolesnikov",
            "email": "nikitakls@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "league/flysystem": "^2.0",
        "league/flysystem-aws-s3-v3": "^2.5"
    },
    "require-dev": {},
    "conflict": {
        "league/flysystem-aws-s3-v3": "<1.0.26"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "nikitakls\\flysystem\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
